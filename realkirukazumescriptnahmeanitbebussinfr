local animationId = "rbxassetid://10928237540"
local animationSpeed = 3 -- Change this value to set the desired speed

-- Assign the AnimationId

-- Load the animation
local animation = Instance.new("Animation")
animation.AnimationId = animationId

-- Get the player's character and humanoid
local character = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Create an Animator if it doesn't exist
local animator = humanoid:FindFirstChildOfClass("Animator") or Instance.new("Animator", humanoid)

-- Load the animation into the animator
local animationTrack = animator:LoadAnimation(animation)

-- Set the speed and play the animation
animationTrack:AdjustSpeed(animationSpeed)
animationTrack:Play()

