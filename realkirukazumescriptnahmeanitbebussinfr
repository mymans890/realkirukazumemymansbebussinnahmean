-- First script
local function executeFirstScript()
-- Options
local KeyToPress = "C" -- Right click while holding C
local StyleName = "Self-Taught Dragon" -- Style name
local StyleColor = Color3.fromRGB(255,211,50) -- Style color

local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://9426968107" -- 9426968107
sound.Looped = true
sound.Parent = workspace
sound.Volume = 0.6
sound:Play()
getfenv().Activated = true

spawn(function()
    while true do
        task.wait()
        if not Activated then break end
        FillHeat()
    end
end)

local plr = game.Players.LocalPlayer
local pgui = plr.PlayerGui
local interf = pgui.Interface
local bt = interf.Battle
local main = bt.Main

local menu = pgui.MenuUI.Menu 
local abil = menu.Abilities.Frame.Frame.Frame

local function sendNotification(text, color, sound)
    if not color then color = Color3.new(1, 1, 1) end
    pgui.NotifyUI.Awards.ChildAdded:Once(function(c)
        if c.Text == text then
            c.TextColor3 = color
            coroutine.wrap(function()
                local con;
                con = game:GetService("RunService").RenderStepped:Connect(function()
                    if not c then
                        con:Disconnect()
                        return
                    end
                    c.TextColor3 = color
                end)()
            end)()
        end
    end)
    notifyevent:Fire(text, sound or nil)
end
 
local alreadyRunning = game.ReplicatedStorage:FindFirstChild("DragonStyle")
if alreadyRunning then
	sendNotification("Dragon Style is already loaded")
	return
end
                                              
