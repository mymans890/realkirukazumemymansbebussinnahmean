-- Function to add the Cancelable folder to a given taunt
local function addCancelableFolder(tauntName)
    local taunt = game.ReplicatedStorage.Moves:FindFirstChild(tauntName)
    if taunt then
        local cancelableFolder = Instance.new("Folder")
        cancelableFolder.Name = "Cancelable"
        cancelableFolder.Parent = taunt
    else
        warn(tauntName .. " not found in ReplicatedStorage.Moves")
    end
end

-- Add Cancelable folder to RushTaunt and BeastTaunt
addCancelableFolder("RushTaunt")
addCancelableFolder("GoonTaunt")

