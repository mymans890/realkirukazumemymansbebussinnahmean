sendNotification("Hold [G] to instantly fill heat.")
 
-- Detect when the G key is pressed to start filling heat
UserInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.G then
        getfenv().Activated = true
    end
end)

-- Detect when the G key is released to stop filling heat
UserInputService.InputEnded:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.G then
        getfenv().Activated = false
    end
end)

